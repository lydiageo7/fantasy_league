INSERT INTO Playing_Match (playing_match_id, home_team_id, guest_team_id, score_home_team, score_guest_team)
SELECT playing_match_id, 
       ((ROW_NUMBER() OVER()) % 18) + 1 AS home_team_id,
       (CASE WHEN ((ROW_NUMBER() OVER()) % 18) + 1 = 18 THEN 1 ELSE ((ROW_NUMBER() OVER()) % 18) + 2 END) AS guest_team_id,
       CAST(substr(result, 1, 1) AS INTEGER),
       CAST(substr(result, 3, 1) AS INTEGER)
FROM Match WHERE date LIKE '2025-05-%';

INSERT INTO Goal (match_id, player_scored, minute, team_scored) 
SELECT match_id, 1, 15, home_team_id 
FROM Playing_Match
WHERE score_home_team &gt; 0;

INSERT INTO Participation (match_id, player_participated, team) 
SELECT playing_match_id, 1, home_team_id FROM Playing_Match
UNION ALL
SELECT playing_match_id, 2, guest_team_id FROM Playing_Match;</sql><sql name="SQL 2*">INSERT INTO Playing_Match (playing_match_id, home_team_id, guest_team_id, score_home_team, score_guest_team) VALUES
